<html lang="en">

<head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Document</title>


    <!-- Latest compiled and minified CSS -->
    <link rel="stylesheet" href="https://cdn.jsdelivr.net/npm/bootstrap@4.6.1/dist/css/bootstrap.min.css">

    <!-- jQuery library -->
    <script src="https://cdn.jsdelivr.net/npm/jquery@3.6.0/dist/jquery.slim.min.js"></script>

    <!-- Popper JS -->
    <script src="https://cdn.jsdelivr.net/npm/popper.js@1.16.1/dist/umd/popper.min.js"></script>

    <!-- Latest compiled JavaScript -->
    <script src="https://cdn.jsdelivr.net/npm/bootstrap@4.6.1/dist/js/bootstrap.bundle.min.js"></script>
</head>

<body>
    <div class="container">
        <div class="row">
            <h1>Create User</h1>

            <div class="col-12">
                <form action="/put-crud" method="POST">
                    <div class="form-group">
                        <label for="inputEmail4">Email</label>
                        <input type="email" value="<%= User.email %>" class="form-control" name="email"
                            placeholder="Email">
                    </div>
                    <div class="form-group">
                        <label for="inputAddress">firstName</label>
                        <input type="text" value="<%= User.firstName %>" class="form-control" name="firstName"
                            placeholder="firstName">
                    </div>
                    <div class="form-group">
                        <label for="inputAddress">lastName</label>
                        <input type="text" value="<%= User.lastName %>" class="form-control" name="lastName"
                            placeholder="lastName">
                    </div>
                    <div class="form-group">
                        <label for="inputCity">phoneNumber</label>
                        <input type="text" value="<%= User.phonenumber %>" name="phonenumber" class="form-control"
                            placeholder="phoneNumber" id="inputCity">
                    </div>
                    <input type="text" hidden value="<%= User.id %>" name="id">

                    <button type="submit" class="btn btn-primary">Sign in</button>
                </form>
            </div>
        </div>
    </div>
</body>

</html>